{
  "sourceType": "module",
  "opts": {
    "recursive": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "include": [ "src/", "index.js" ]
  },
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": [ "jsdoc" ]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}